{
  "name": "angular-template",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run postinstall:web",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall && npx electron-builder install-app-deps",
    "electron:windows": "npm run electron:build && npx electron-builder build --windows",
    "electron:linux": "npm run electron:build && npx electron-builder build --linux",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:build": "npm run postinstall:electron && npm run electron:serve-tsc && npm run build -- --base-href /dashboard",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "ng": "ng",
    "start": "ng serve",
    "build": "node ./build/build-pre.js && ng build --prod --aot --base-href /app/ --output-path dist/app --deploy-url /app/",
    "debug": "ng build --base-href /",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:update": "jest --updateSnapshot",
    "server": "node server.js",
    "e2e": "ng e2e"
  },
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@auth0/auth0-spa-js": "^1.7.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ng-select/ng-select": "^2.6.0",
    "@ngx-formly/bootstrap": "^5.6.1",
    "@ngx-formly/core": "^5.6.1",
    "@ngx-progressbar/core": "^5.3.1",
    "@ngx-progressbar/http": "^5.3.1",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@ngxs/form-plugin": "^3.2.0",
    "@ngxs/logger-plugin": "^3.1.4-dev.master-b50211a",
    "@ngxs/router-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.3.2",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.11",
    "bootstrap": "^4.1.3",
    "ckeditor4-angular": "^1.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "datatables.net": "^1.10.19",
    "datatables.net-bs4": "^1.10.19",
    "datatables.net-responsive": "^2.0.0",
    "datatables.net-rowreorder": "^1.2.6",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ngx-google-places-autocomplete": "^2.0.4",
    "ngx-lightbox": "^1.2.0",
    "ngx-toastr": "^10.0.4",
    "popper.js": "^1.14.4",
    "prismjs": "^1.15.0",
    "pusher-js": "^4.3.1",
    "request": "^2.88.0",
    "resumablejs": "^1.1.0",
    "rxjs": "^6.3.0",
    "tablesaw": "^3.1.2",
    "web-animations-js": "^2.3.1",
    "xterm": "^3.6.0",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@ckeditor/ckeditor5-editor-inline": "^18.0.0",
    "@types/datatables.net": "^1.10.13",
    "@types/googlemaps": "^3.39.3",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^23.3.12",
    "@types/jquery": "^3.3.6",
    "@types/node": "~10.12.18",
    "codelyzer": "^4.4.4",
    "electron": "^4.0.1",
    "electron-builder": "20.38.4",
    "electron-reload": "1.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "json-loader": "^0.5.7",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "^5.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.1",
    "typescript": "^3.2.2",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.1",
    "yaml-import-loader": "^1.3.6"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "^@app/shared$": "<rootDir>/src/app/shared/",
      "^@app/shared/(.*)$": "<rootDir>/src/app/shared/$1",
      "^@app/external/(.*)$": "<rootDir>/src/app/external/$1",
      "^@app/external$": "<rootDir>/src/app/external",
      "^@app/state/(.*)$": "<rootDir>/src/app/state/$1",
      "^@env/(.*)$": "<rootDir>/src/environments/$1",
      "^@views/(.*)$": "<rootDir>/src/app/views/$1"
    }
  }
}
