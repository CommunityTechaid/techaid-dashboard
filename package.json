    "ng:lint": "ng lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:update": "jest --updateSnapshot",
    "server": "node server.js",
    "e2e": "ng e2e"
